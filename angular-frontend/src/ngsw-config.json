{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    },
    {
      "name": "dating-assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/images/avatars/**",
          "/assets/images/icons/**",
          "/assets/images/backgrounds/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "/api/v1/profiles/me",
        "/api/v1/soul-connections/active",
        "/api/v1/revelations/timeline/*",
        "/api/v1/users/me"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "10s"
      }
    },
    {
      "name": "api-freshness",
      "urls": [
        "/api/v1/messages/*",
        "/api/v1/notifications",
        "/api/v1/soul-connections/discover"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "5m",
        "timeout": "5s"
      }
    },
    {
      "name": "api-background-sync",
      "urls": [
        "/api/v1/analytics/events",
        "/api/v1/soul-connections/viewed",
        "/api/v1/revelations/progress"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 20,
        "maxAge": "30m"
      }
    },
    {
      "name": "user-photos",
      "urls": [
        "https://cdn.dinner1.com/photos/**",
        "/api/v1/photos/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,
        "maxAge": "1d"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**"
  ],
  "navigationRequestStrategy": "performance"
}